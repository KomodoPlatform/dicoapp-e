    "css-loader": "^1.0.1",
    "detect-port": "^1.2.3",
    "electron": "^3.0.7",
    "electron-builder": "^20.31.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.2",
    "enzyme": "^3.7.0",
